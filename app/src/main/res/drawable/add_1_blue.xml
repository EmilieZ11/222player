<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <size
                android:width="96dp"
                android:height="96dp" />
        </shape>
    </item>
    <item
        android:bottom="40dp"
        android:top="40dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/colorPrimary" />
            <size
                android:width="96dp"
                android:height="16dp" />
            <corners android:radius="4dp" />
        </shape>
    </item>
    <item
        android:left="40dp"
        android:right="40dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/colorPrimary" />
            <size
                android:width="16dp"
                android:height="96dp" />
            <corners android:radius="4dp" />
        </shape>
    </item>
</layer-list>