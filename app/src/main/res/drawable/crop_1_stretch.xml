<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <size
                android:width="100dp"
                android:height="100dp" />
        </shape>
    </item>
    <item
        android:left="15dp"
        android:right="15dp">
        <shape android:shape="rectangle">
            <stroke
                android:width="5dp"
                android:color="#FFF" />
            <corners android:radius="10dp" />
        </shape>
    </item>
    <item
        android:bottom="12dp"
        android:left="22dp"
        android:right="22dp"
        android:top="12dp">
        <shape android:shape="rectangle">
            <stroke
                android:width="5dp"
                android:color="#FFF" />
        </shape>
    </item>
</layer-list>